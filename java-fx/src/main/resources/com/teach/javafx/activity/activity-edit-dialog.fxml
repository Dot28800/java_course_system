<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.teach.javafx.controller.ActivityEditController">
   <top>
        <GridPane BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="95.0" minWidth="10.0" prefWidth="47.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="153.0" minWidth="10.0" prefWidth="113.33333333333334" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="179.99999999999997" minWidth="10.0" prefWidth="175.99999999999997" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="136.0" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="141.0" minWidth="10.0" prefWidth="141.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="95.0" minWidth="10.0" prefWidth="23.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="63.0" minHeight="10.0" prefHeight="63.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="98.0" minHeight="10.0" prefHeight="98.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="86.0" minHeight="10.0" prefHeight="86.0" vgrow="SOMETIMES" />
                <RowConstraints />
            </rowConstraints>
            <Label text="在此处修改学生信息" GridPane.columnIndex="2" GridPane.rowIndex="1">
                <font>
                    <Font size="18.0" />
                </font>
            </Label>
            <Label text="活动地点" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2">
                <font>
                    <Font size="24.0" />
                </font>
            </Label>
            <TextField fx:id="activityPlaceField" prefHeight="30.0" prefWidth="100.0" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            <Label text="活动内容" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="3">
                <font>
                    <Font size="24.0" />
                </font>
            </Label>
            <TextField fx:id="contentField" prefHeight="33.0" prefWidth="100.0" GridPane.columnIndex="2" GridPane.rowIndex="3" />
            <Label GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Label text="活动时间" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="4">
                <font>
                    <Font size="24.0" />
                </font>
            </Label>
            <DatePicker fx:id="timePicker" prefHeight="33.0" prefWidth="176.0" GridPane.columnIndex="2" GridPane.rowIndex="4" />
            <Label GridPane.columnIndex="3" GridPane.rowIndex="1" />
            <Label text="活动评价" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowIndex="2">
                <font>
                    <Font size="24.0" />
                </font>
            </Label>
            <TextField fx:id="activityEvaluationField" prefHeight="30.0" prefWidth="100.0" GridPane.columnIndex="4" GridPane.rowIndex="2" />
            <Label text="活动类型" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowIndex="3">
                <font>
                    <Font size="24.0" />
                </font>
            </Label>
            <Button mnemonicParsing="false" onAction="#okButtonClick" text="确定" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="5">
                <font>
                    <Font size="24.0" />
                </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#cancelButtonClick" text="取消" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowIndex="5">
                <font>
                    <Font size="24.0" />
                </font>
            </Button>
            <ComboBox fx:id="typeComboBox" prefHeight="33.0" prefWidth="141.0" GridPane.columnIndex="4" GridPane.rowIndex="3" />
        </GridPane>
   </top>

</BorderPane>
